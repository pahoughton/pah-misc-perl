<?xml version="1.0" encoding="US-ASCII"?>

<!DOCTYPE rel-notes
	PUBLIC "-//DocTools//DTD Release Notes//EN" 
	       "dtd/DocTools/rel-notes.dtd" [

 <!ENTITY TOP.DIR ".">
 <!ENTITY % projectEntity
	PUBLIC "-//Project//ENTITY Project Variables//EN"
	       "./dtd/project.ent">
 %projectEntity;
]>

<?xml-stylesheet
    href="xsl/DocTools/html/rel-notes.xsl"
    type="text/xsl" ?>

<rel-notes>
  <dochead>
    <title>
       &PRJ.NAME; Release Notes
    </title>
    <subtitle>
       
    </subtitle>
    <docdesc>
      This is the release notes for &PRJ.NAME;. It contains a
      summarized history of each release which includes the release
      dates, status and the changes that were made.
    </docdesc>
    <keywords>
       
    </keywords>
    <authorgrp>
      <author fullname="Paul Houghton"
	      email="Paul.Houghton@wcom.com"/>
    </authorgrp>
    <projectname>
      %PRJ.NAME;
    </projectname>
    <docversion
        created="02/13/01 08:56"
	verlastmodby="$Author$"
        verlastmod="$Date$"
        verversion="$Revision$"
	verstatus="$State$"
        verversionid="$Id$"/>
    <contact>&PRJ.XML.CONTACT;</contact>
  </dochead>
  <release>
    <version status="prod">1.02.01</version>
    <date>8/27/02</date>
    <desc>
      <p>
	Added Teamnet and Text::CommaNum modules.
      </p>
      <dl>
	<dt><doc usedesc="no" doc="Devel/Teamnet">Teamnet</doc></dt>
	<dd>
	  Perl module to access WorldCom Teamnet LDAP server.
	</dd>
	<dt><doc usedesc="no"
		 doc="Devel/CommaNum">Text::CommaNum</doc></dt>
	<dd>
	  Perl module to add commas to numbers.
	</dd>
      </dl>
    </desc>
  </release>
  <release>
    <version status="devel">1.01.01</version>
    <date>06/15/01</date>
    <ver-mgmt>
      Dimmensions - COS/INFR_OBJS Rel: &PRJ.NAME; &PRJ.VER;
    </ver-mgmt>
    <desc>
      <p>
	Initial Release - includes the following:
      </p>
      <dl>
	<dt><doc usedesc="no" doc="Devel/Options">App::Options</doc></dt>
	<dd>
	  Perl module to process command line arguments.
	</dd>
	<dt><doc usedesc="no" doc="Devel/Debug">App::Debug</doc></dt>
	<dd>
	  Functions to generate Debug output from perl scripts.
	</dd>
	<dt><doc usedesc="no" doc="Devel/Self">Doc::Self</doc></dt>
	<dd>
	  Provides complete self documentation of Perl Scripts.
	</dd>
	<dt><doc usedesc="no" doc="Devel/ParseCxx">ParseCxx</doc></dt>
	<dd>
	  C++ header file parser.
	</dd>
	<dt><doc usedesc="no" doc="Devel/Fill">Text::Fill</doc></dt>
	<dd>
	  Fill and wrap mutilple paragraphs.
	</dd>
      </dl>
    </desc>
  </release>
</rel-notes>

